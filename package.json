{
  "name": "apollo-backend-starter",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "build": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --progress",
    "prebuild": "rm -rf build/*",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node server",
    "start-dev": "nodemon --watch 'server/**/*.ts' --exec 'ts-node' server/index.ts",
    "migrate": "knex migrate:latest --knexfile server/db/knexfile.js",
    "seed": "knex seed:run  --knexfile server/db/knexfile.js"
  },
  "devDependencies": {
    "jimp": "^0.2.27",
    "sqlite": "^2.3.0"
  },
  "dependencies": {
    "@types/body-parser": "^1.16.2",
    "@types/compression": "^0.0.33",
    "@types/cookie-parser": "^1.3.30",
    "@types/express-jwt": "^0.0.34",
    "@types/jsonwebtoken": "^7.2.0",
    "@types/jwt-decode": "^1.4.28",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.12",
    "@types/randomstring": "^1.1.5",
    "apollo-client": "^1.0.0-rc.8",
    "assets-webpack-plugin": "^3.5.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.1",
    "compression": "1.6.2",
    "cookie-parser": "^1.4.3",
    "express": "4.15.2",
    "express-jwt": "^5.1.0",
    "graphql": "^0.9.1",
    "graphql-server-express": "^0.6.0",
    "graphql-tag": "^2.0.0",
    "graphql-tools": "^0.10.1",
    "graphql-typings": "0.0.1-beta-2",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^7.3.0",
    "jwt-decode": "^2.1.0",
    "knex": "^0.12.8",
    "morgan": "1.8.1",
    "objection": "^0.7.8",
    "randomstring": "^1.1.5",
    "ts-node": "^3.0.2",
    "tslint": "^4.5.1",
    "tslint-config-airbnb": "^1.1.1",
    "typescript": "^2.2.2"
  }
}
